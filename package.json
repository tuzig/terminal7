{
  "name": "terminal7",
  "version": "1.4.2",
  "description": "A touchable terminal emulator & multiplexer tablet app",
  "main": "src/index.js",
  "homepage": "https://terminal7.dev",
  "private": false,
  "build": {
    "appId": "dev.terminal7",
    "files": [
      "index.html",
      "css/*"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "dependencies": {
    "@capacitor/android": "^4.6.0",
    "@capacitor/app": "^4.1.1",
    "@capacitor/browser": "^1.0.7",
    "@capacitor/cli": "^4.6.0",
    "@capacitor/clipboard": "^4.1.0",
    "@capacitor/core": "^4.6.0",
    "@capacitor/device": "^4.1.0",
    "@capacitor/ios": "^4.6.0",
    "@capacitor/keyboard": "^4.1.0",
    "@capacitor/network": "^4.1.0",
    "@capacitor/preferences": "^4.0.2",
    "@capacitor/status-bar": "^4.1.1",
    "@capgo/capacitor-purchases": "^2.0.24",
    "@robingenz/capacitor-background-task": "^1.0.0",
    "@tuzig/codemirror": "5.65.5-mods",
    "@tuzig/toml": "^3.0.0-browser",
    "@types/marked": "^4.0.7",
    "capacitor-native-biometric": "^4.1.3",
    "capacitor-rate-app": "^3.1.0-beta.1",
    "capacitor-ssh-plugin": "^0.3.0",
    "ed25519-keygen": "^0.2.4",
    "glob": "<8",
    "hammerjs": "<3",
    "idb": "7.1.1",
    "marked": "^4.1.0",
    "uuid": "^8.3.2",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-image": "^0.4.0",
    "xterm-addon-search": "^0.11.0",
    "xterm-addon-web-links": "^0.8.0",
    "xterm-addon-webgl": "^0.14.0",
    "@liveconfig/xterm-webfont": "^2.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.20.1",
    "@typescript-eslint/eslint-plugin": "<6",
    "@typescript-eslint/parser": "<6",
    "eslint": "<9",
    "jsdom": "^19.0.0",
    "typescript": "<5",
    "vite": "^4.0.0",
    "vite-plugin-pwa": "^0.11.13",
    "vitest": "^0.22.1",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "stage": "vite build --sourcemap && vite preview",
    "run": "cap run ios",
    "test": "vitest",
    "build": "vite build -c vite.dev.config.js",
    "clean": "rm -rf dist ios/App/App/public/*",
    "aatp": "bash ./aatp/run",
    "lint": "npx eslint aatp src --ext .js,.jsx,.ts,.tsx",
    "capacitor:copy:before": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuzig/terminal7.git"
  },
  "keywords": [
    "terminal",
    "tmux",
    "webrtc"
  ],
  "author": "Benny Daon",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/tuzig/terminal7/issues"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": true
    },
    "globals": {
      "db": true,
      "terminal7": true
    },
    "rules": {
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "no-constant-condition": [
        "error",
        {
          "checkLoops": false
        }
      ]
    }
  }
}
