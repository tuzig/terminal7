{
  "name": "terminal7",
  "version": "0.18.1",
  "description": "A touchable terminal emulator & multiplexer tablet app",
  "main": "src/index.js",
  "private": true,
  "bin": {
    "terminal7": "download_browser.js"
  },
  "build": {
    "appId": "dev.terminal7",
    "files": [
      "index.html",
      "css/*"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "dependencies": {
    "@capacitor/android": "^3.2.5",
    "@capacitor/app": "^1.0.5",
    "@capacitor/cli": "<4",
    "@capacitor/clipboard": "^1.0.5",
    "@capacitor/core": "<4",
    "@capacitor/device": "^1.0.5",
    "@capacitor/ios": "<4",
    "@capacitor/network": "^1.0.5",
    "@capacitor/storage": "^1.2.2",
    "@iarna/toml": "<3",
    "@robingenz/capacitor-background-task": "^1.0.0",
    "ansi-escapes": "<6",
    "codemirror": "git+https://github.com/tuzig/CodeMirror.git",
    "glob": "<8",
    "hammerjs": "<3",
    "idb": "<7",
    "npm": "<7",
    "uuid": "<9",
    "web": "0.0.2",
    "websocket-extensions": ">=0.1.4",
    "workbox-webpack-plugin": "^6.3.0",
    "xterm": "git+https://github.com/tuzig/xterm.js.git#semver:<5.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-search": "^0.7.0",
    "xterm-webfont": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "css-loader": "^5.2.4",
    "file-loader": "^6.1.0",
    "karma": "^6.3.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-source-map-support": "^1.4.0",
    "karma-webpack": "^4.0.2",
    "mocha": "<8",
    "puppeteer": "^3.1.0",
    "serve": "^12.0.1",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.1.4",
    "ts-loader": "<9",
    "typescript": "<5",
    "url-loader": "^4.1.0",
    "webpack": "<5",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "<4",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "start": "npm run build && npx serve -p 8000 www",
    "test": "karma start --single-run",
    "build": "npx webpack --output www/terminal7.bundle.js && npx cap copy",
    "postinstall": "cd ./node_modules/xterm && yarn install && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuzig/terminal7.git"
  },
  "keywords": [
    "terminal",
    "javascript",
    "tmux",
    "webrtc"
  ],
  "author": "Benny Daon",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/tuzig/terminal7/issues"
  },
  "homepage": "https://github.com/tuzig/terminal7#readme",
  "mocha": {
    "watch-files": [
      "src/*.js",
      "test/*.js"
    ]
  }
}
